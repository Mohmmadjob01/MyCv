@model IEnumerable<Project>


      <div id="Projects">
        <div class="container content-lg">
            <div class="row margin-b-40">
                <div class="col-sm-6">
                    <h2>Projects</h2>
                    @foreach (var i in Model)
                {
                    <h4>@i.ProjectTitle</h4>
                    <p>@i.ProjectDescription</p>
                    <p>Click Here To See The Project --> <a href="@i.ProjectLink">@i.ProjectTitle</a>  </p>
                }
                </div>
            </div>
                </div>
            </div>